  author = {Upmeier zu Belzen, Annette and Krüger, Dirk},
  date = {2010},
  pages = {41--57},
  keywords = {Anwendungsperspektive,Herstellungsperspektive,Modell,Modellkompetenz,Modellobjekt},
  file = {Upmeier zu Belzen & Krüger 2010.pdf:/Users/chris/Library/Application Support/Zotero/Profiles/ee5w7h0t.default/zotero/storage/NQHSKA66/Upmeier zu Belzen & Krüger 2010.pdf:application/pdf},
